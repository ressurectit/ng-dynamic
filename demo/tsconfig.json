{
    "compilerOptions":
    {
        "target": "es2020",
        "module": "es2020",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": false,
        "noImplicitReturns": false,
        "noImplicitOverride": true,
        "noImplicitThis": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "strict": false,
        "strictNullChecks": false,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "importHelpers": true,
        "moduleResolution": "node",
        "outDir": "wwwroot/app",
        "baseUrl": ".",
        "paths": 
        {
            "@anglr/dynamic/layout": ["../layout/src/index.ts"],
            "@anglr/dynamic/layout-editor": ["../layout-editor/src/index.ts"],
            "@anglr/dynamic/layout-relations": ["../layout-relations/src/index.ts"],
            "@anglr/dynamic/form/types": ["../form/src/types.ts"],
            "@anglr/dynamic/form": ["../form/src/index.ts"],
            "@anglr/dynamic/relations": ["../relations/src/index.ts"],
            "@anglr/dynamic/relations-editor": ["../relations-editor/src/index.ts"],
            "@anglr/dynamic/basic-components/relations": ["../basic-components/src/relations.ts"],
            "@anglr/dynamic/basic-components/types": ["../basic-components/src/types.ts"],
            "@anglr/dynamic/basic-components": ["../basic-components/src/index.ts"],
            "@anglr/dynamic/material-components/relations": ["../material-components/src/relations.ts"],
            "@anglr/dynamic/material-components/types": ["../material-components/src/types.ts"],
            "@anglr/dynamic/material-components": ["../material-components/src/index.ts"],
            "@anglr/dynamic/css-components/relations": ["../css-components/src/relations.ts"],
            "@anglr/dynamic/css-components/types": ["../css-components/src/types.ts"],
            "@anglr/dynamic/css-components": ["../css-components/src/index.ts"],
            "@anglr/dynamic/handlebars-components/relations": ["../handlebars-components/src/relations.ts"],
            "@anglr/dynamic/handlebars-components/types": ["../handlebars-components/src/types.ts"],
            "@anglr/dynamic/handlebars-components": ["../handlebars-components/src/index.ts"],
            "@anglr/dynamic/rest-components/relations": ["../rest-components/src/relations.ts"],
            "@anglr/dynamic/rest-components/types": ["../rest-components/src/types.ts"],
            "@anglr/dynamic/rest-components": ["../rest-components/src/index.ts"],
            "@anglr/dynamic/tinymce-components/relations": ["../tinymce-components/src/relations.ts"],
            "@anglr/dynamic/tinymce-components/types": ["../tinymce-components/src/types.ts"],
            "@anglr/dynamic/tinymce-components": ["../tinymce-components/src/index.ts"],
            "@anglr/dynamic": ["../src/index.ts"],
        },
        "lib":
        [
            "es2020",
            "dom"
        ],
        "types":
        [
            "node",
        ],
        "plugins":
        [
            {
                "name": "@angular/language-service"
            }
        ]
    },
    "files":
    [
        "config/global.d.ts"
    ],
    "include":
    [
        "app/**/*.ts",
        "../basic-components/**/*.ts",
        "../material-components/**/*.ts",
        "../css-components/**/*.ts",
        "../handlebars-components/**/*.ts",
        "../rest-components/**/*.ts",
        "../tinymce-components/**/*.ts",
        "../form/**/*.ts",
    ],
    "exclude":
    [
        "node_modules"
    ],
    "angularCompilerOptions":
    {
        "strictInjectionParameters": true,
        "strictTemplates": true
    }
}