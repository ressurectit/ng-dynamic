        "@jscrpt/common": "3.3.0",
        "@ngx-translate/core": "14.0.0",
        "angular2-hotkeys": "13.1.0",
        "@ng-dnd/core": "2.0.2",
        "react-dnd-html5-backend": "16.0.1",
        "zone.js": "0.12.0",
        "rxjs": "7.5.6",
