        },
        "./styles": {
            "sass": "./styles/index.scss"
        },
        "./styles/themes/*": {
            "sass": "./styles/themes/*.scss"
        },
        "./typings/*": {
            "types": "./typings/*/index.d.ts"
        },
        ".": {
            "types": "./src/index.d.ts",
            "esm2022": "./es2022/src/index.js",
            "es2022": "./es2022/src/index.js",
            "node": "./es2022/src/index.js",
            "default": "./es2022/src/index.js"
        },
