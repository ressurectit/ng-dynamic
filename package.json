{
    "name": "@anglr/dynamic",
    "version": "0.0.0-placeholder",
    "description": "Contains code for generating dynamic content from metadata",
    "module": "./dist/es5/dynamic.js",
    "esm2015": "./dist/es2015/dynamic.js",
    "typings": "./dynamic.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "npm run build:dynamic && npm run build:designer && npm run build:components && npm run build:nodes && npm run build:page",
        "build:dynamic": "ngc -p tsconfig.es5.json && ngc -p tsconfig.es2015.json",
        "build:designer": "ngc -p designer/tsconfig.es5.json && ngc -p designer/tsconfig.es2015.json",
        "build:components": "ngc -p components/tsconfig.es5.json && ngc -p components/tsconfig.es2015.json",
        "build:nodes": "ngc -p nodes/tsconfig.es5.json && ngc -p nodes/tsconfig.es2015.json",
        "build:page": "ngc -p page/tsconfig.es5.json && ngc -p page/tsconfig.es2015.json",
        "clean": "rimraf dist tests docs src/**/*.d.ts designer/**/*.d.ts components/**/*.d.ts nodes/**/*.d.ts page/**/*.d.ts */*.d.ts */*.metadata.json ./*.metadata.json ./*.d.ts */dist",
        "clean:duplicates": "rimraf */dist/*/src",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build && npm run clean:duplicates",
        "deploy": "npm publish",
        "deploy:next": "npm publish --tag next",
        "pretest": "webpack",
        "test": "karma start karma.conf.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/ng-dynamic.git"
    },
    "keywords": [
        "angular",
        "ng",
        "utilities"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/ng-dynamic/issues"
    },
    "homepage": "https://github.com/ressurectit/ng-dynamic",
    "peerDependencies": {
        "@angular/core": ">=9.0.0-rc.9",
        "@angular/common": ">=9.0.0-rc.9",
        "@angular/forms": ">=9.0.0-rc.9",
        "@angular/router": ">=9.0.0-rc.9",
        "@angular/platform-browser": ">=9.0.0-rc.9",
        "@angular/cdk": ">=9.0.0-rc.8",
        "rxjs": ">=6.5.0",
        "@jscrpt/common": ">=1.1.0",
        "d3": "^5.15.0",
        "moment": "^2.24.0",
        "monaco-editor": "^0.19.3"
    },
    "devDependencies": {
        "@angular/core": "^9.0.0-rc.9",
        "@angular/common": "^9.0.0-rc.9",
        "@angular/forms": "^9.0.0-rc.9",
        "@angular/router": "^9.0.0-rc.9",
        "@angular/cdk": "^9.0.0-rc.8",
        "rxjs": "^6.5.0",
        "@jscrpt/common": "^1.1.0",
        "d3": "^5.15.0",
        "moment": "^2.24.0",
        "monaco-editor": "^0.19.3",
        "@anglr/types": "1.1.18",
        "@types/node": "13.1.7",
        "@types/d3": "5.7.2",
        "@angular/compiler-cli": "^9.0.0-rc.9",
        "@angular/compiler": "^9.0.0-rc.9",
        "@angular/language-service": "^9.0.0-rc.9",
        "@angular/platform-browser-dynamic": "^9.0.0-rc.9",
        "@angular/platform-browser": "^9.0.0-rc.9",
        "tsickle": "0.38.0",
        "@compodoc/compodoc": "1.1.11",
        "rimraf": "3.0.0",
        "copyfiles": "2.1.0",
        "replace-in-file": "5.0.2",
        "typescript": "3.7.4",
        "npm-git-version": "1.1.1"
    }
}
