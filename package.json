            "esm2020": "./es2020/layout-editor/src/index.js",
            "es2020": "./es2020/layout-editor/src/index.js",
            "es2015": "./es2015/layout-editor/src/index.js",
            "node": "./es2015/layout-editor/src/index.js",
            "default": "./es2020/layout-editor/src/index.js"
        },
        "./layout-relations": {
